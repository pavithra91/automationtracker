@model AutomationTracker.Models.AssetList
@{
    ViewBag.Title = "ViewVOIP";
    ViewBag.Menu = "ViewVOIP";
    Layout = "~/Views/Shared/_SystemLayout.cshtml";
}

<section class="common_section">
    <div class="container">

        <div class="container" style="padding-bottom:20px;">
            <input type="button" style="float:right;" class="btn btn-primary" value="Add VOIP Devices" onclick="location.href='@Url.Action("ManageVOIP", "Asset", new { id = 0 })'" />
        </div>

        <table id="example" class="display" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Unit ID</th>
                    <th>Unit Type Name</th>
                    <th>Model Name</th>
                    <th>Asset Number</th>
                    <th>Serial number</th>
                    <th>Extention No</th>
                    <th>Remarks</th>
                    <th>Current User</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var items in Model.voipList)
                {
                    <tr>
                        <td>
                            @items.AUTOID
                        </td>
                        <td>
                            @items.UnitType1.UnitTypeName
                        </td>
                        <td>
                            @items.ModelType1.ModelName
                        </td>
                        <td>
                            @items.AssestNo
                        </td>
                        <td>
                            @items.SerialNo
                        </td>
                        <td>
                            @items.ExtentionNo
                        </td>
                        <td>
                            @items.Remarks
                        </td>
                        <td>
                            @items.ActualAssignee
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "ManageVOIP", new { id = items.AUTOID })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>

