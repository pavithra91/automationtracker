@model AutomationTracker.Models.AssetList
@{
    ViewBag.Title = "ViewComputers";
    Layout = "~/Views/Shared/_SystemLayout.cshtml";
}

<section class="common_section">
    <div class="container">
        <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Unit ID</th>
                    <th>Unit Type Name</th>
                    <th>Model Name</th>
                    <th>Asset Number</th>
                    <th>Serial number</th>
                    <th>Software Version</th>
                    <th>Current User</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var items in Model.computerList)
                {
                    <tr>
                        <td>
                            @items.AUOTID
                        </td>
                        <td>
                            @items.UnitType1.UnitTypeName
                        </td>
                        <td>
                            @items.ModelType1.ModelName
                        </td>
                        <td>
                            @items.AssestNo
                        </td>
                        <td>
                            @items.SerialNo
                        </td>
                        <td>
                            @*@items.Software.SoftwareName*@
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "EditComputers", new { id = items.AUOTID })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>


